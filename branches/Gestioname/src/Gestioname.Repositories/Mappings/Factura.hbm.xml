<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"   
                   assembly="Gestioname.DomainModel"                    
                   namespace="Gestioname.DomainModel">
  <class name ="Factura">
    <id name ="Id">
      <generator class="identity"/>
    </id>
    <property name="NumeroFactura" />
    <property name="FueImpresa" />
    <property name="EsNotaDeCredito" />
    <property name="Observaciones" />
    <property name="Fecha" />
    <property name="EsCotizacion" />
    <property name="ValorDolar" />
    <many-to-one name="Orden" class="Factura" column="IdOrden"/>
    

  </class>
</hibernate-mapping>