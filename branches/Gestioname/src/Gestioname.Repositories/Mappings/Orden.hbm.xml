<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Gestioname.DomainModel"
                   namespace="Gestioname.DomainModel">
  <class name ="Orden">
    <id name ="Id">
      <generator class="identity"/>
    </id>
    <many-to-one name="Cliente" class="Cliente" column="IdCliente" />
    <property name="FechaEmision" />
    <property name="FechaEntrega" />
    <property name="Numero" />
    <property name="Observaciones" />
    <property name="DescuentoEspecial" />
    <bag name="Items" lazy ="true" cascade="all">
      <key column="Id"></key>
      <one-to-many class="OrdenItem"/>
    </bag>
  </class>
</hibernate-mapping>
