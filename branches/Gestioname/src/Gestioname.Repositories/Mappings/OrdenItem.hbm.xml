<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Gestioname.DomainModel"
                   namespace="Gestioname.DomainModel">
  <class name ="OrdenItem">
    <id name ="Id">
      <generator class="identity"/>
    </id>
    <many-to-one name="Articulo" class="Articulo" column="IdArticulo" />
    <property name="Cantidad" />
    <property name="Descuento" />
    <many-to-one name="Id" column="Id" class="Orden" ></many-to-one>
  </class>
</hibernate-mapping>
